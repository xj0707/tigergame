<script type="text/javascript">
    $("#but").click(function(){
        if($("input[name='userid']").val()==''){
            alert('用户ID不能为空');
            $("input[name='userid']").focus();
        }else{
            document.myfrom.submit();
        }
    });
//    $("#but1").click(function(){
//        if($("input[name='account']").val()==''){
//            alert('账号不能为空');
//            $("input[name='account']").focus();
//        }else{
//            document.myfrom1.submit();
//        }
//    });
    $("#but2").click(function(){
        if($("input[name='name']").val()==''){
            alert('昵称不能为空');
            $("input[name='name']").focus();
        }else{
            document.myfrom2.submit();
        }
    });
    $("#but3").click(function(){
        if($("input[name='conis']").val()==''){
            alert('昵称不能为空');
            $("input[name='coins']").focus();
        }else{
            document.myfrom3.submit();
        }
    });
    function getuserid($id){
        $("#user_id").val($id);
    }
    // 提交表单
    function check_form()
    {
       // var jinbi = $.trim($('#jinbi').val());
        var zuansi     = $.trim($('#zuansi').val());
        var userid=$("#user_id").val();
        if(!userid)
        {
            alert('操作异常！请重新操作');
            return false;
        }
//        if(!jinbi && !zuansi){
//            alert('你还没有选择金币或钻石');
//            return false;
//        }
        if(!zuansi){
            alert('钻石数量不能为空');
            return false;
        }
        if(zuansi==0){
            alert('钻石数量不能为0');
            return false;
        }
        var form_data = $('#form_data').serialize();
        // 异步提交数据到action/add_action.php页面
        $.ajax(
                {
                    url: "<?php echo url('addcoins'); ?>?userid="+userid,
                    data:{"form_data":form_data},
                    type: "post",
                    beforeSend:function()
                    {
                        $("#tip").html("<span style='color:blue'>正在处理...</span>");
                        return true;
                    },
                    success:function(data)
                    {
                        if(data > 0)
                        {
                            //$("#tip").html("<span style='color:blueviolet'>恭喜操作成功！</span>");
                            // document.location.href='system_notice.php'
                            alert("操作成功");
                            location.reload();
                        }
                        else
                        {
                           // $("#tip").html("<span style='color:red'>失败，请重试</span>");
                            alert('操作失败');
                        }
                    },
                    error:function()
                    {
                        alert('请求出错');
                    },
                    complete:function()
                    {
                        $('#acting_tips').hide();
                    }
                });

        return false;
    }

</script>


